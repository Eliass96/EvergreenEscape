{
  "name": "dam.tfg.evergreenescape",
  "displayName": "EvergreenEscape",
  "version": "1.0.0",
  "description": "EvergreenEscape the game.",
  "main": "server/app.js",
  "scripts": {
    "run": "node server/app.js",
    "dev": "nodemon --exec server/app.js",
    "comp-perfil": "pug -c -D -n crearPerfil -o www/js/pug profile.pug",
    "comp-puntuaciones": "pug -c -D -n crearPuntuaciones -o www/js/pug rankings.pug",
    "comp-puntuacionesPais": "pug -c -D -n crearPuntuacionesPorPais -o www/js/pug countriesRankings.pug",
    "comp-puntuacionesGlobales": "pug -c -D -n crearPuntuacionesGlobales -o www/js/pug globalRankings.pug"
  },
  "keywords": [
    "ecosystem:cordova"
