  "author": "Elías Botía García y Jesús Robles García",
  "license": "Apache-2.0",
  "dependencies": {
    "@capacitor/android": "^5.7.4",
    "@capacitor/ios": "^5.7.4",
    "@ionic-native/camera": "^5.3.0",
    "@ionic-native/core": "^5.3.0",
    "@ionic-native/file": "^5.3.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "gradle": "^1.2.4",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.0",
    "phaser": "^3.80.1",
    "phaser3-rex-plugins": "^1.80.1",
    "pug": "^3.0.2",
    "sweetalert2": "^11.10.7"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {}
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "nodemon": "^3.1.0"
  }
